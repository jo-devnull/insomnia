minecraft {
  "#forge:armors"
  "#forge:armor/helmets"
  "#forge:armor/chestplates"
  "#forge:armor/leggings"
  "#forge:armor/boots"
  "#minecraft:trimmable_armor"
}

immersive_armors {
  for (armor in List(
    "bone",
    "wither",
    "warrior",
    "heavy",
    "robe",
    "slime",
    "divine",
    "prismarine",
    "wooden",
    "steampunk"
  )) {
    "\(armor)_helmet"
    "\(armor)_chestplate"
    "\(armor)_leggings"
    "\(armor)_boots"
  }
}

