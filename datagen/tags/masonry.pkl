create {
  for (material in List(
    "granite",
    "diorite",
    "andesite",
    "calcite",
    "dripstone",
    "deepslate",
    "tuff",
    "asurine",
    "crimsite",
    "limestone",
    "ochrum",
    "scoria",
    "scorchia",
    "veridium"))
  {
    "cut_\(material)"
    "polished_cut_\(material)"
    "cut_\(material)_bricks"
    "small_\(material)_bricks"
    "layered_\(material)"
  }
}

supplementaries {
  "slidy_block"
  "gravel_bricks"

  for (type in List("stone_tile", "ash_bricks", "blackstone_tile")) {
    type
    "\(type)_stairs"
    "\(type)_slab"
    "\(type)_wall"
  }
}

createdieselgenerators {
  "asphalt_block"
  "asphalt_stairs"
  "asphalt_slab"
}

createdeco {
  for (type in List("verdant", "red", "dean", "pearl", "umber", "scarlet", "dusk", "blue")) {
    "\(type)_bricks"
    "short_\(type)_bricks"
    "tiled_\(type)_bricks"
    "long_\(type)_bricks"
    "corner_\(type)_bricks"
    "cracked_\(type)_bricks"
    "mossy_\(type)_bricks"

    for (block in List("stairs", "slab", "wall")) {
      "\(type)_brick_\(block)"
      "short_\(type)_brick_\(block)"
      "tiled_\(type)_brick_\(block)"
      "long_\(type)_brick_\(block)"
      "cracked_\(type)_brick_\(block)"
      "mossy_\(type)_brick_\(block)"
    }
  }
}
